<script>
	$(function() {

		$(".tt-weixin").click(function() {

			webUtil.jDialog.Modal("weixinDialog", "weixinEntry");

		});

		//浏览器	

		if ($.browser.msie && $.browser.version == "6.0") {
			//帮助微软消灭ie6

			$('body')
					.append(
							'<div class="master"><p>您的浏览器是<strong>IE6.0</strong>，漏洞较多，用户体验较差，微软官方已放弃支持，为了自身电脑安全和获取最佳用户体验建议你根据自身需求升级至<a href="http://windows.microsoft.com/zh-CN/internet-explorer/downloads/ie-8" target="_blank" class="red"><strong>IE8.0</strong></a>以上版本或者使用<a href="http://firefox.com.cn/" target="_blank" class="red"><strong>火狐</strong></a>浏览器</p></div><div class="m-close m-close-short">关闭</div>');
			$(".master").delay(1000).slideDown('', function() {
				$(".m-close").fadeIn();
			});
			$(".m-close-short").click(function() {
				$(".m-close").fadeOut('', function() {
					$(".master").slideUp();
				});
			});
			$(".m-close-long").click(function() {
				$(".m-close").fadeOut('', function() {
					$(".master").slideUp();
					$.cookie('masterShow', 'hidden');
				});
			});
		}

	})
</script>
